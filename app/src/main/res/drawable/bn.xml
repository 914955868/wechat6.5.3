<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/ca" />
            <solid android:color="@color/br" />
            <stroke android:width="@dimen/cb" android:color="@color/bu" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/ca" />
            <solid android:color="@color/bt" />
            <stroke android:width="@dimen/cb" android:color="@color/bw" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/ca" />
            <solid android:color="@color/bs" />
            <stroke android:width="@dimen/cb" android:color="@color/bv" />
        </shape>
    </item>
</selector>