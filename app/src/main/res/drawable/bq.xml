<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/ca" />
            <solid android:color="@color/c7" />
            <stroke android:width="@dimen/cb" android:color="@color/bx" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/ca" />
            <solid android:color="@color/c9" />
            <stroke android:width="@dimen/cb" android:color="@color/bz" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/ca" />
            <solid android:color="@color/c8" />
            <stroke android:width="@dimen/cb" android:color="@color/by" />
        </shape>
    </item>
</selector>